class Animal {
    public void sound() { System.out.println("Animal sound"); }
}

class Dog extends Animal {
    public void sound() { System.out.println("Bark"); }
}

public class Activity6 {
    public static void main(String[] args) {
        Animal a = new Animal();
        Animal d = new Dog();
        a.sound();
        d.sound();
    }
}
